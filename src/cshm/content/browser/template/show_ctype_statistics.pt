
<style>
.title{
    display: flex;
    justify-content: space-around;
}
p{
    font-size: 22px;
}
</style>
<form action="download_ctype_excel" method="post">
  <input type='hidden' name="data" value="${view/json_data}">
  <input type='hidden' name="course" value="${view/course}">
  <input type='hidden' name="period" value="${view/period}">

  <input type='submit' value="Download Excel">
</form>

<p id="json_data" style='display:none'>${view/json_data}</p>


<table>
  <tr colspan=2>
    <th colspan=2>已填人數 / 總人數 = 回收率</th>
  </tr>
  <tr colspan=2>
    <tal:cond condition='view/rate'>
      <th colspan=2>${view/count} / ${view/numbers} = ${view/rate}</th>
    </tal:cond>
    <tal:cond condition='not: view/rate'>
      <th colspan=2>
        <a href="${view/abs_url}/edit">尚未設定總人數</a>
      </th>
    </tal:cond>
  </tr>

  <tr>
    <th>參訓目的</th>
    <th>年齡</th>
  </tr>
  <tr>
   <td id="pie2"></td>
   <td id="pie3"></td>
 </tr>
  <tr>
    <th>行業別</th>
    <th>您是如何知道本項訓練課程:</th>
  </tr>
  <tr>
   <td id="pie4"></td>
   <td id="pie5"></td>
 </tr>
  <tr>
    <th>您選擇本中心得因素(複選)</th>
    <th>職業安全衛生法之中央主管機關為何單位</th>
  </tr>
  <tr>
   <td id="pie6"></td>
   <td id="pie7"></td>
 </tr>
  <tr>
    <th>勞動契約係以下列何種目的為正確</th>
    <th>丙種職業安全衛生業務主管式用於僱用多少人以下之事業單位</th>
  </tr>
  <tr>
   <td id="pie8"></td>
   <td id="pie9"></td>
 </tr>

</table>
<script>
$(document).ready(function () {
    json_data = $('#json_data')[0].innerText
    data = JSON.parse(json_data)
    var pie_2 = []
    var pie_3 = []
    var pie_4 = []
    var pie_5 = []
    var pie_6 = []
    var pie_7 = []
    var pie_8 = []
    var pie_9 = []
    var pie_10 = []
    var pie_11 = []
    var pie_12 = []
    var pie_13 = []
    var pie_14 = []

    for(let[key,value] of Object.entries(data['2'])){
	    pie_2.push([key,value])
    }
    for(let[key,value] of Object.entries(data['3'])){
	    pie_3.push([key,value])
    }
    for(let[key,value] of Object.entries(data['4'])){
	    pie_4.push([key,value])
    }
    for(let[key,value] of Object.entries(data['5'])){
	    pie_5.push([key,value])
    }
    for(let[key,value] of Object.entries(data['6'])){
	    pie_6.push([key,value])
    }
    for(let[key,value] of Object.entries(data['7'])){
	    pie_7.push([key,value])
    }
    for(let[key,value] of Object.entries(data['8'])){
	    pie_8.push([key,value])
    }
    for(let[key,value] of Object.entries(data['9'])){
	    pie_9.push([key,value])
    }


    var chart = c3.generate({
        bindto: '#pie2',
        size: {
            width: 350,height: 350,
        },
        data: {
            // iris data from R
            columns: pie_2,
            type : 'pie',
        }
    });
    var chart = c3.generate({
        bindto: '#pie3',
        size: {
            width: 350,height: 350,
        },
        data: {
            // iris data from R
            columns: pie_3,
            type : 'pie',
        }
    });
    var chart = c3.generate({
        bindto: '#pie4',
        size: {
            width: 350,height: 350,
        },
        data: {
            // iris data from R
            columns: pie_4,
            type : 'pie',
        }
    });
    var chart = c3.generate({
        bindto: '#pie5',
        size: {
            width: 350,height: 350,
        },
        data: {
            // iris data from R
            columns: pie_5,
            type : 'pie',
        }
    });
    var chart = c3.generate({
        bindto: '#pie6',
        size: {
            width: 350,height: 350,
        },
        data: {
            // iris data from R
            columns: pie_6,
            type : 'pie',
        }
    });
    var chart = c3.generate({
        bindto: '#pie7',
        size: {
            width: 350,height: 350,
        },
        data: {
            // iris data from R
            columns: pie_7,
            type : 'pie',
        }
    });
    var chart = c3.generate({
        bindto: '#pie8',
        size: {
            width: 350,height: 350,
        },
        data: {
            // iris data from R
            columns: pie_8,
            type : 'pie',
        }
    });
    var chart = c3.generate({
        bindto: '#pie9',
        size: {
            width: 350,height: 350,
        },
        data: {
            // iris data from R
            columns: pie_9,
            type : 'pie',
        }
    });
});

</script>
